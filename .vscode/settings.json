{
	"biome.enabled": true,
	  "biome.lsp.bin": {
    "win32-x64": "./node_modules/.pnpm/@biomejs+biome@2.2.0/node_modules/@biomejs/cli-win32-x64/biome.exe",
    "linux-x64": "./node_modules/.pnpm/@biomejs+biome@2.2.0/node_modules/@biomejs/cli-linux-x64/biome",
    "linux-arm64": "./node_modules/.pnpm/@biomejs+biome@2.2.0/node_modules/@biomejs/cli-linux-arm64/biome",
    "linux-x64-musl": "./node_modules/.pnpm/@biomejs+biome@2.2.0/node_modules/@biomejs/cli-linux-x64-musl/biome",
    "linux-arm64-musl": "./node_modules/.pnpm/@biomejs+biome@2.2.0/node_modules/@biomejs/cli-linux-arm64-musl/biome",
    "darwin-x64": "./node_modules/.pnpm/@biomejs+biome@2.2.0/node_modules/@biomejs/cli-darwin-x64/biome",
    "darwin-arm64": "./node_modules/.pnpm/@biomejs+biome@2.2.0/node_modules/@biomejs/cli-darwin-arm64/biome"
  },
	"editor.formatOnSave": true,
	"prettier.enable": false,
	"eslint.enable": false,
	"editor.defaultFormatter": "biomejs.biome",

	"editor.codeActionsOnSave": {
		"source.fixAll.biome": "explicit",
		"source.organizeImports.biome": "explicit"
	},

	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
}
